[Unit]
Description=Yarra Server
Documentation=http://yarra.rocks
After=smbd.service
Requires=smbd.service

[Service]
Type=simple

User=yarraserver
WorkingDirectory=/home/yarraserver/yarra/webgui
ExecStart=/home/yarraserver/yarra/webgui/YarraWebGUI

Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target
